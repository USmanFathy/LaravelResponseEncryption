{
    "name": "usman-ahmed/laravel-response-encryption",
    "description": "Encrypt Laravel API JSON responses automatically using Laravel's Crypt system.",
    "license": "MIT",

    "autoload": {
        "psr-4": {
            "UsmanAhmed\\LaravelResponseEncryption\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Usman Ahmed Fathy",
            "email": "usmanahmedfathy@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "UsmanAhmed\\LaravelResponseEncryption\\ResponseEncryptionServiceProvider"
            ],
            "aliases": {
                "ResponseEncryption": "UsmanAhmed\\LaravelResponseEncryption\\Facades\\ResponseEncryption"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0"
    }
}
